partial default xkb_types "default" {

    type "CTRL_QWERTY" {

        modifiers = None+Shift+Control;

        map[None] = Level1;
        map[Shift] = Level2;
        map[Control] = Level3;
        map[Control+Shift] = Level4;

        level_name[Level1]= "Base";
        level_name[Level2]= "Shift";
        level_name[Level3]= "Modifiers";
        level_name[Level4]= "Modifiers+Shift";

        // Fix bug on terminal (Shift+n opens new terminal)
        preserve[Control] = Control;
    };
};
